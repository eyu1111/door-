-- 设置密钥
local correctKey = "3291165933"

-- 从 URL 中获取密钥参数
local url = "https://raw.githubusercontent.com/eyu1111/door-/refs/heads/main/%E5%AE%8732911165933"
local key = string.match(url, "key=(%w+)")

-- 验证密钥
if key ~= correctKey then
    error("Unauthorized access: Invalid key.")
    return
end

-- 加载主脚本
local mainScriptUrl = "https://raw.githubusercontent.com/notpoiu/mspaint/main/main.lua"
loadstring(game:HttpGet(mainScriptUrl))()
